    </None>
    <None Update="Data\Json\Sushi.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="Data\Json\SushiInBasket.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
